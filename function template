#include <iostream>
using namespace std;
template <typename T>
T max(T a, T b) {
return (a > b) ? a : b;
}
int main() {
int intMax = max(5, 10);
double doubleMax = max(3.14, 2.71);
cout<<"YUDHEEKSHANA - LAB EXERCISE 10 - FUNCTION TEMPLATE"<<endl;
cout << "Maximum of 5 and 10 is: " << intMax << endl;
cout << "Maximum of 3.14 and 2.71 is: " << doubleMax <<endl;
return 0;
}
